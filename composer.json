{
    "name": "mobtexting/mobtexting-laravel",
    "description": "A boilerplate for contributions.",
    "homepage": "https://github.com/mobtexting/mobtexting-laravel",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "mobtexting/sdk": "^1.0",
        "illuminate/notifications": "^7|^8|^9.0",
        "illuminate/support": "^7|^8|^9.0",
        "illuminate/events": "^7|^8|^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Mobtexting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Mobtexting\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Mobtexting\\MobtextingProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
